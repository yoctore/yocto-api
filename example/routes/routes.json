{
  "routes" : [
    {
      "path"              : "/users",
      "alias"             : ["/user"],
      "model"             : "user",
      "requestExcluded"   : ["put", "delete" , "patch"]
    },
    {
      "path"                : "/users/:user_id",
      "alias"               : ["/user/:user_id"],
      "model"               : "user",
      "paramToRetrieve"     : ["user_id"],
      "requestExcluded"     : ["post"]
    },
    {
      "path"              : "/availability",
      "alias"             : ["/availability"],
      "model"             : "availability",
      "requestExcluded"   : ["put", "delete" , "patch"]
    },
    {
      "path"                : "/availabilitys/:availability_id",
      "alias"               : ["/availabilies/:availability_id"],
      "model"               : "availability",
      "paramToRetrieve"     : ["availability_id"],
      "requestExcluded"     : ["post"]
    }
  ]
}
